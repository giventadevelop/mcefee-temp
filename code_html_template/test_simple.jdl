// Very simple test JDL file
service all with serviceImpl
paginate all with pagination
dto all with mapstruct
search * with no
filter all

// Explicitly set all entities to same service
microservice * with eventmanagement

// Test with just two simple entities
entity Event {
    id Long,
    title String required maxlength(255)
}

entity Media {
    id Long,
    title String required maxlength(255),
    eventId Long
}

// Simple relationship
relationship ManyToOne {
    Media{event} to Event
}



































































