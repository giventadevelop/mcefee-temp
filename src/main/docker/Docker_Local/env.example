# Environment Variables for Docker Compose Local Development
# Copy this file to .env and update the values as needed

# JWT API Authentication Variables
# These are used for backend API authentication
JWT_API_AUTH_USERNAME=admin
JWT_API_AUTH_PASSWORD=admin123

# Database Configuration (Optional - defaults are set in docker-compose.yml)
POSTGRES_USER=malayalees_user
POSTGRES_PASSWORD=malayalees_password
POSTGRES_DB=malayalees_db

# Application Configuration
SPRING_PROFILES_ACTIVE=dev
SERVER_PORT=8080

# Logging Configuration
LOGGING_LEVEL_ROOT=INFO
LOGGING_LEVEL_COM_MALAYALEES=DEBUG

# Optional: Redis Configuration
REDIS_PASSWORD=
